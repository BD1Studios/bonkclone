{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
		"linebreak-style": "off",
		"max-len": "off",
		"require-jsdoc": "off",
		"new-cap": "off",
		"no-unused-vars": "off"
    }
}
